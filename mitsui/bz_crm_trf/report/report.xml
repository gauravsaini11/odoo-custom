<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="report_trf_pdf" model="ir.actions.report">
        <field name="name">TRF PDF</field>
        <field name="model">trf</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">bz_crm_trf.report_trf_pdf1</field>
        <field name="report_file">bz_crm_trf.report_trf_pdf1</field>
        <field name="binding_model_id" ref="model_trf"/>
