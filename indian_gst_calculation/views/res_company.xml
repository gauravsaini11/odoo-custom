<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="res_company_gst_number_add" model="ir.ui.view">
      <field name="name">res.company.gst.number.add</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='company_registry']" position="before">
          <field name="company_gst_number" string="Company GSTIN" /> 
        </xpath>
      </field>
    </record>
  </data>
</odoo>