                                    />
                                    <field name="thermal_resistance_between_diode_junction"
                                           attrs="{'invisible': [('process_type', 'not in', ['bypass_diode_thermal_test'] )]}"
                                    />
                                    <field name="temperature_during_recovery_period"
                                           attrs="{'invisible': [('process_type', 'not in', ['damp_test'] )]}"
                                    />
                                    <field name="max_allowed_cycle_time"
                                           attrs="{'invisible': [('process_type', 'not in', ['humidity_test','thermal_test'] )]}"
                                    />
                                    <field name="temperature_during_humidity_period"
                                           attrs="{'invisible': [('process_type', 'not in', ['humidity_test'] )]}"
                                    />
                                    <field name="total_no_hours"
                                           attrs="{'invisible': [('process_type', 'not in', ['thermal_test'] )]}"
                                    />
                                    <field name="no_of_thermal_cycles"
                                           attrs="{'invisible': [('process_type', 'not in', ['thermal_test'] )]}"
                                    />
                                    <field name="time_duration_s"
                                           attrs="{'invisible': [('process_type', 'not in', ['conduit_test'] )]}"
                                    />
                                    <field name="vmmp"
                                           attrs="{'invisible': [('process_type', 'not in', ['determination_test','performance_af_test',
                                    'performance_stc_test','performance_nmot_test','bifaciality_test'
                                    ] )]}"
                                    />
                                    <field name="max_system_voltage"
                                           attrs="{'invisible': [('process_type', 'not in', ['wet_leakage_current'] )]}"
                                    />
                                    <field name="name_plate_verification"
                                           attrs="{'invisible': [('process_type', 'not in', ['visual_inspection'] )]}"
                                    />
                                    <field name="current"
                                           attrs="{'invisible': [('process_type', 'not in', ['wet_leakage_current'] )]}"
                                    />
                                </group>
                            </group>
                        </page>
                        <page name="equipment_details" string="Equipment Details">
<!--                            <form string="Equipment List">-->
                                <sheet>
                                    <field name="equipment_list_ids">
                                        <tree editable="bottom">
                                            <field name="equipment_name"/>
                                            <field name="equipment_id"/>
                                            <field name="calibration_due_date"/>
                                        </tree>
                                    </field>
                                </sheet>
<!--                            </form>-->
                        </page>
                        <page name="description" string="Result Compilation">
                            <field name="description" nolabel="1" placeholder="Describe your task..." type="html"/>
                            <div class="d-none oe_clear"/>
                        </page>
                        <page string="Remarks" name="remarks">
                            <group>
                                <field name="remarks"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
                <!-- Chatter box -->
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="activity_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
